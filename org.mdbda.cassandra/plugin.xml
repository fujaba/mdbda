<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.mdbda.diagrameditor.pattern">
      <GraphicalFeatures
            addFeatureClass="org.mdbda.cassandra.AddCassandraResourceFeature"
            createFeatureClass="org.mdbda.cassandra.CreateCassandraResourceFeature"
            group="Resources"
            typeId="CassandraResource">
      </GraphicalFeatures>
   </extension>
   <extension
         id="CassandraResource"
         point="org.mdbda.codegen.plugin">
      <ResourceCodeStylePlugin
            CodeStyle="Hadoop"
            ResourceTemplateClass="org.mdbda.cassandra.codegen.CassandraResourceHadoop">
      </ResourceCodeStylePlugin>
   </extension>

</plugin>
