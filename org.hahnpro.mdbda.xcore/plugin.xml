<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDAWorkflow -->
      <package
            uri="org.hahnpro.mdbda.model.workflow"
            class="org.hahnpro.mdbda.model.workflow.WorkflowPackage"
            genModel="model/MDBDAWorkflow.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDA -->
      <package
            uri="org.mdbda.model"
            class="org.mdbda.model.ModelPackage"
            genModel="model/MDBDA.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated MDBDA -->
      <factory
            uri="org.mdbda.model"
            class="org.mdbda.model.provider.ModelItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated MDBDA -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.mdbda.model.presentation.ModelModelWizardID"
            name="%_UI_ModelModelWizard_label"
            class="org.mdbda.model.presentation.ModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ModelModelFile.gif">
         <description>%_UI_ModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated MDBDA -->
      <editor
            id="org.mdbda.model.presentation.ModelEditorID"
            name="%_UI_ModelEditor_label"
            icon="icons/full/obj16/ModelModelFile.gif"
            extensions="mdbdamodel"
            class="org.mdbda.model.presentation.ModelEditor"
            contributorClass="org.mdbda.model.presentation.ModelActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDAPattern -->
      <package
            uri="org.hahnpro.mdbda.model.pattern"
            class="org.hahnpro.mdbda.model.pattern.PatternPackage"
            genModel="model/MDBDAPattern.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDASummarizationPattern -->
      <package
            uri="org.hahnpro.mdbda.model.pattern.summatization"
            class="org.hahnpro.mdbda.model.pattern.summatization.SummatizationPackage"
            genModel="model/MDBDASummarizationPattern.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDAJoinPattern -->
      <package
            uri="org.hahnpro.mdbda.model.pattern.join"
            class="org.hahnpro.mdbda.model.pattern.join.JoinPackage"
            genModel="model/MDBDAJoinPattern.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDADataOrganizationPattern -->
      <package
            uri="org.hahnpro.mdbda.model.pattern.dataorganization"
            class="org.hahnpro.mdbda.model.pattern.dataorganization.DataorganizationPackage"
            genModel="model/MDBDADataOrganizationPattern.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDAResources -->
      <package
            uri="org.hahnpro.mdbda.model.resources"
            class="org.hahnpro.mdbda.model.resources.ResourcesPackage"
            genModel="model/MDBDAResources.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated MDBDAFilteringPattern -->
      <package
            uri="org.hahnpro.mdbda.model.pattern.filtering"
            class="org.hahnpro.mdbda.model.pattern.filtering.FilteringPackage"
            genModel="model/MDBDAFilteringPattern.xcore"/>
   </extension>

</plugin>
