package org.hahnpro.mdbda.model.pattern


import org.hahnpro.mdbda.model.resources.Resource
import org.hahnpro.mdbda.model.workflow.Workflow

interface Pattern extends Resource{
	container Workflow workflow opposite pattern
	refers Pattern[] dependsOn
}

interface SimplePattern extends Pattern{
	refers Resource inputResource
	refers Resource outputResource
}

interface JoinPattern extends Pattern{
	refers Resource[] inputResources
	refers Resource[] outputResources
}