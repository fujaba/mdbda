   </extension>
   <extension
         point="org.mdbda.diagrameditor.pattern">
      <GraphicalFeatures
            addFeatureClass="org.mdbda.hdfs.AddHDFSResourceFeature"
            createFeatureClass="org.mdbda.hdfs.CreateHDFSResourceFeature"
            group="Resources"
            typeId="HDFSResource">
      </GraphicalFeatures>
   </extension>

</plugin>
